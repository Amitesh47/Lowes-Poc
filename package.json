{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon  --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --mode='development' --config webpack.server.js --watch",
    "dev:build-client": "webpack --mode='development' --config webpack.client.js --watch",
    "prod": "npm-run-all --parallel prod-dev:*",
    "prod-dev:server": "nodemon  --watch build --exec node build/bundle.js",
    "prod-dev:build-server": "webpack --mode='production' --config webpack.server.js --watch",
    "prod-dev:build-client": "webpack --mode='production' --config webpack.client.js --watch",
    "test": "jest",
    "storybook": "start-storybook "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "concurrently": "3.5.0",
    "express": "^4.17.1",
    "react": "16.8.6",
    "react-bootstrap": "^1.0.0-beta.10",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "serialize-javascript": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "babel-cli": "6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "7.1.2",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.19",
    "nodemon": "^1.19.1",
    "npm-run-all": "4.1.1",
    "react-redux": "^7.1.0",
    "redux-mock-store": "^1.5.3",
    "redux-saga": "^1.0.5",
    "styled-components": "^4.3.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.21.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
